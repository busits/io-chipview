<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="IoChipTagView">
        <attr name="chip_background_color" format="color" />
        <attr name="chip_text_color" format="color" />
        <attr name="chip_close_color" format="color" />
        <attr name="chip_closable" format="boolean" />
        <attr name="chip_elevation" format="integer" />
        <attr name="chip_style" format="enum">
            <enum name="circular_with_image" value="0" />
            <enum name="circular_text_only" value="1" />
            <enum name="rect_with_image" value="2" />
            <enum name="rect_text_only" value="3" />
        </attr>
    </declare-styleable>

</resources>